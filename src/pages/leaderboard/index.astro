// Sort printers by score in descending order
const sortedPrinters = [...printers].sort((a, b) => b.score - a.score);
---

<Layout title="3D Printer Leaderboard" description="Top rated 3D printers">
  <main class="max-w-7xl mx-auto px-6 py-12">
    <h1 class="text-4xl font-bold mb-8 dm-serif">3D Printer Leaderboard</h1>
    
    <div class="grid gap-6">
      {sortedPrinters.map((printer, index) => (
        <Card>
          <div class="flex flex-col md:flex-row justify-between gap-4">
            <div>
              <div class="flex items-center gap-4 mb-2">
                <span class="text-2xl font-bold">#{index + 1}</span>
                <h2 class="text-2xl font-bold">{printer.name}</h2>
              </div>
              <p class="text-gray-700">{printer.review}</p>
            </div>
            <div class="flex items-start">
              <div class="bg-black text-white px-4 py-2 rounded-lg">
                <span class="text-2xl font-bold">{printer.score.toFixed(1)}</span>
                <span class="text-sm">/10</span>
              </div>
            </div>
          </div>
        </Card>
      ))}
    </div>
  </main>
</Layout>
