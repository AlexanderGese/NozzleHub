---
// Define a variable to fetch the email list data.
const emailList = await import('../static/email_list.json');
---

<div class="email-list">
  <h2>Our Subscribers</h2>
  <ul>
    {emailList.map((subscriber) => (
      <li key={subscriber.email}>
        <strong>{subscriber.name}</strong> - <a href={`mailto:${subscriber.email}`}>{subscriber.email}</a>
      </li>
    ))}
  </ul>
</div>
